{
  "root": "libs/frontend/shared/rx-websocket-wrapper",
  "sourceRoot": "libs/frontend/shared/rx-websocket-wrapper/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/frontend/shared/rx-websocket-wrapper",
        "main": "libs/frontend/shared/rx-websocket-wrapper/src/index.ts",
        "tsConfig": "libs/frontend/shared/rx-websocket-wrapper/tsconfig.lib.json",
        "assets": ["libs/frontend/shared/rx-websocket-wrapper/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/frontend/shared/rx-websocket-wrapper/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/frontend/shared/rx-websocket-wrapper"],
      "options": {
        "jestConfig": "libs/frontend/shared/rx-websocket-wrapper/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["frontend"]
}

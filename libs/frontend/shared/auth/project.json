      "outputs": ["coverage/libs/frontend/shared/auth"],
      "options": {
        "jestConfig": "libs/frontend/shared/auth/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["scope:frontend", "scope:detective-client", "type:feature-auth"]
}

{
  "version": 1,
  "projects": {
    "detective-client": {
      "projectType": "application",
      "root": "apps/detective-client",
      "sourceRoot": "apps/detective-client/src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/detective-client",
            "index": "apps/detective-client/src/index.html",
            "main": "apps/detective-client/src/main.ts",
            "polyfills": "apps/detective-client/src/polyfills.ts",
            "tsConfig": "apps/detective-client/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "libs/shared/assets/src",
                "output": "./"
              },
              {
                "glob": "**/*",
                "input": "libs/shared/assets/src/assets",
                "output": "assets"
              }
            ],
            "styles": ["libs/shared/styles/src/index.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "libs/shared/environments/src/lib/environment.ts",
                  "with": "libs/shared/environments/src/lib/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "detective-client:build:production"
            },
            "development": {
              "browserTarget": "detective-client:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "detective-client:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/detective-client/src/**/*.ts", "apps/detective-client/src/**/*.html"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/detective-client"],
          "options": {
            "jestConfig": "apps/detective-client/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["shared-assets", "shared-styles"]
    },
    "detective-client-e2e": {
      "root": "apps/detective-client-e2e",
      "sourceRoot": "apps/detective-client-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/detective-client-e2e/cypress.json",
            "devServerTarget": "detective-client:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "detective-client:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/detective-client-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["detective-client"]
    },
    "shared-assets": {
      "projectType": "library",
      "root": "libs/shared/assets",
      "sourceRoot": "libs/shared/assets/src",
      "architect": {},
      "tags": ["scope:shared", "type:assets"]
    },
    "shared-components": {
      "projectType": "library",
      "root": "libs/shared/components",
      "sourceRoot": "libs/shared/components/src",
      "prefix": "detective-frontend",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/components"],
          "options": {
            "jestConfig": "libs/shared/components/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/components/src/**/*.ts", "libs/shared/components/src/**/*.html"]
          }
        }
      },
      "tags": ["scope:shared", "type:components"]
    },
    "shared-data-access": {
      "projectType": "library",
      "root": "libs/shared/data-access",
      "sourceRoot": "libs/shared/data-access/src",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/data-access"],
          "options": {
            "jestConfig": "libs/shared/data-access/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/data-access/src/**/*.ts", "libs/shared/data-access/src/**/*.html"]
          }
        }
      },
      "tags": ["scope:shared", "type:data-access"]
    },
    "shared-environments": {
      "projectType": "library",
      "root": "libs/shared/environments",
      "sourceRoot": "libs/shared/environments/src",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/environments/src/**/*.ts", "libs/shared/environments/src/**/*.html"]
          }
        }
      },
      "tags": ["scope:shared", "type:environments"]
    },
    "shared-styles": {
      "projectType": "library",
      "root": "libs/shared/styles",
      "sourceRoot": "libs/shared/styles/src",
      "architect": {},
      "tags": ["scope:shared", "type:styles"]
    }
  }
}
